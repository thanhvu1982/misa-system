{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "allowJs": false,
    "sourceMap": false,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "src",
  ],
  "exclude": [
    "dist",
    "app"
  ]
}